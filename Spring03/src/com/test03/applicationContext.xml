<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<!-- 홍길동,250,개발팀 -->

	<bean id="hong" class="com.test03.Developer">
		<constructor-arg name="name" value="홍길동" />
		<constructor-arg name="salary" value="250" />
		<property name="dept" value="개발" />
	</bean>
	
	<!-- 이순신,200,기술팀 -->
	<bean id="lee" class="com.test03.Engineer">
		<constructor-arg name="name" value="이순신" />
		<constructor-arg name="salary" value="200" />
		<property name="dept" value="기술" />
	</bean>
	
	
	
	
</beans>
